    </servlet>
    <servlet>
        <servlet-name>FinalBillServlet</servlet-name>
        <servlet-class>hr.algebra.servlet.FinalBillServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteProductServlet</servlet-name>
        <url-pattern>/delete-product</url-pattern>
